atMessages.slimScroll({ height: 210, color: '#fff', size: '3px', position: 'left', touchScrollStep: 100 });	$('a', chatUsers).click(function(){	to_user_id = $(this).attr('data-to-id');	to_user_nama = $(this).attr('data-to-nama');	chat_active = 1;	update_chat();	$("#ito_user_id").val(to_user_id);	$("#ito_user_nama").val(to_user_nama);	$("#to_user_nama").html(to_user_nama);	var fd = {};	fd.to_user_id = to_user_id;	fd.last_pesan_id = 0;	$.post('http://jangiman.local/api_chat/pm_get',fd).done(function(data){	if(data.status == 100 || data.status == '100'){	var h = '';	$.each(data.result,function(key,val){	if(iterator%7==1){	h += '<li class="text-center"><small>'+val.cdate+'</small></li>';	}	if(val.from_user_id == from_user_id){	h += '<li class="chat-talk-msg chat-talk-msg-highlight themed-border animation-slideLeft">'+val.pesan+'</li>';	}else{	h += '<li class="chat-talk-msg animation-slideRight">'+val.pesan+'</li>';	}	last_pesan_id = val.id;	iterator++;	});	$("#chat_conversation_list").html(h);	}else{	$("#global_message_danger_text").html('Tidak dapat mengambil chat list');	$("#global_message_danger").show();	}	chatUsers.slideUp();	chatTalk.slideDown();	chatMessages.slimScroll({ scrollTo: chatMessages[0].scrollHeight + 'px' });	chatInput.focus();	});	return false;	});	$('#chat-talk-close-btn').click(function(){	chatTalk.slideUp();	chatUsers.slideDown();	chat_active = 0;	$("#chat_conversation_list").html('');	return false;	});	$('#sidebar-chat-form').submit(function(e){	var fd = new FormData($(this)[0]);	var url = 'http://jangiman.local/api_chat/pm_send/';	chatMsg = chatInput.val();	if (chatMsg) {	$.ajax({	type: 'POST',	url: url,	data: fd,	processData: false,	contentType: false,	success: function(respon){	if(respon.status == 100 || respon.status == '100'){	}else{	$("#global_message_danger_text").html('Pesan tidak terkirim');	$("#global_message_danger").show();	}	chatInput.val('');	update_chat();	},	error:function(){	$("#global_message_danger_text").html('Pesan tidak terkirim');	$("#global_message_danger").show();	}	});	}	e.preventDefault();	});	};	$.get("http://jangiman.local/api_chat").done(function(data){	if(data.status == '100' || data.status == 100){	var user = data.result.user;	from_user_id = user.id;	from_user_nama = user.nama;	var h ='';	$.each(data.result.users,function(key,val){	if(val.id == from_user_id) return true;	if(val.is_online){	h += '<li><a href="javascript:void(0)" class="chat-user-online" title="'+val.nama+'" data-from-id="'+from_user_id+'" data-from-nama="'+from_user_nama+'" data-to-id="'+val.id+'" data-to-nama="'+val.nama+'"><span></span><img src="http://jangiman.local/skin/admin/img/placeholders/avatars/avatar12.jpg" alt="'+val.nama+'" class="img-circle"></a></li>';	}else{	h += '<li><a href="javascript:void(0)" class="chat-user-busy" title="'+val.nama+'" data-from-id="'+from_user_id+'" data-from-nama="'+from_user_nama+'" data-to-id="'+val.id+'" data-to-nama="'+val.nama+'"><span></span><img src="http://jangiman.local/skin/admin/img/placeholders/avatars/avatar12.jpg" alt="'+val.nama+'" class="img-circle"></a></li>';	}	});	$("#chat_user_list").html(h);	chatUi();	}else{	}	}).fail(function(){	$("#global_message_danger_text").html('Chat system currently unavailable...');	$("#global_message_danger").show();	});	function update_chat(){	if(chat_active == 1 || chat_active == "1"){	var chatUsers = $('.chat-users');	var chatTalk = $('.chat-talk');	var chatMessages = $('.chat-talk-messages');	var chatInput = $('#sidebar-chat-message');	var chatMsg = '';	var fd = {};	fd.to_user_id = to_user_id;	fd.last_pesan_id = last_pesan_id;	$.post('http://jangiman.local/api_chat/pm_get',fd).done(function(data){	if(data.status == 100 || data.status == '100'){	var h = '';	$.each(data.result,function(key,val){	if(iterator%7==1){	h += '<li class="text-center"><small>'+val.cdate+'</small></li>';	}	if(val.from_user_id == from_user_id){	h += '<li class="chat-talk-msg chat-talk-msg-highlight themed-border animation-slideLeft">'+val.pesan+'</li>';	}else{	h += '<li class="chat-talk-msg animation-slideRight">'+val.pesan+'</li>';	}	last_pesan_id = val.id;	iterator++;	});	if(last_pesan_id == "0" || last_pesan_id == 0){	$("#chat_conversation_list").html(h);	}else{	$("#chat_conversation_list").append(h);	}	}else{	$("#global_message_danger_text").html('Tidak dapat mengambil chat list');	$("#global_message_danger").show();	}	chatUsers.slideUp();	chatTalk.slideDown();	chatMessages.slimScroll({ scrollTo: chatMessages[0].scrollHeight + 'px' });	chatInput.focus();	});	}else{	}	setTimeout(function(){	update_chat();	},4666);	}	});</script></body></html>